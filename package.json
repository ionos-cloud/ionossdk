{
  "name": "@ionos-cloud/ionossdk",
  "description": "Ionos Cloud SDK tool",
  "version": "1.3.0",
  "author": "Florin Mihalache",
  "bin": {
    "ionossdk": "./bin/run"
  },
  "bugs": "https://github.com/ionos-cloud/ionossdk/issues",
  "devDependencies": {
    "@oclif/test": "^3.2.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "chai": "^4.3.4",
    "eslint": "^8.57.0",
    "eslint-config-oclif": "^5.2.0",
    "eslint-config-oclif-typescript": "^3.1.0",
    "mocha": "^10.2.0",
    "oclif": "^4.15.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ionos-cloud/ionossdk",
  "keywords": [
    "oclif",
    "sdk",
    "ionos"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ionossdk",
    "dirname": "ionossdk",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ionos-cloud/ionossdk",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "build": "rm -rf lib && tsc -b",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && oclif readme",
    "test": "mocha --require ts-node/register --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^6.2.0",
    "axios": "^1.7.0",
    "chalk": "^4.1.2",
    "execa": "^5.1.1",
    "tslib": "^2.8.0",
    "yaml": "^2.6.0"
  }
}
